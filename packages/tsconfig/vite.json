{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "Display": "Vite",
  "compilerOptions": {
    "incremental": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "lib": ["es2020", "DOM", "WebWorker", "DOM.Iterable"],
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "types": ["vite/client"]
  }
}
